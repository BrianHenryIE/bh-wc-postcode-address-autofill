{
  "phpVersion": "7.4",
  "mappings": {
    "wp-content/themes": "./wp-content/themes",
    "wp-content/plugins/woocommerce": "./wp-content/plugins/woocommerce",
    "wp-content/plugins/basic-auth": "./wp-content/plugins/basic-auth",
    "wp-content/plugins/gutenberg": "./wp-content/plugins/gutenberg",
    "wp-content/plugins/woo-gutenberg-products-block": "./wp-content/plugins/woo-gutenberg-products-block",
    "wp-content/plugins/woocommerce-gateway-dummy": "./wp-content/plugins/woocommerce-gateway-dummy",
    "wp-cli.yml": "./wp-cli.wp-env.yml"
  },
  "port": 8888,
  "testsPort": 8889,
  "lifecycleScripts": {
    "afterStart": "./tests/e2e-pw/setup/initialize-external.sh"
  },
  "env": {
    "development": {
      "plugins": [
        "./wp-content/plugins/basic-auth",
        "./wp-content/plugins/woocommerce",
        "."
      ]
    },
    "tests": {
      "mappings": {
        "e2e-setup": "./tests/e2e-pw/setup"
      },
      "plugins": [
        "./wp-content/plugins/basic-auth",
        "./wp-content/plugins/woocommerce"
      ]
    }
  }
}
